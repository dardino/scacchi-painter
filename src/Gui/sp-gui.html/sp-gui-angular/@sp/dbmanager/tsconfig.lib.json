{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "declarationMap": true,
    "target": "es2015",
    "declaration": true,
    "inlineSources": true,
    "composite": true,
    "types": [
      "wicg-file-system-access"
    ],
    "lib": [
      "dom",
      "es2018",
      "ESNext"
    ]
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": true,
    "enableResourceInlining": true
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}

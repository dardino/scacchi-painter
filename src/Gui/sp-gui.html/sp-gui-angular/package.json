{
  "name": "sp-gui-angular",
  "version": "0.0.0",
  "scripts": {
    "debugger": "yarn remotedebug_ios_webkit_adapter --port 9000",
    "tunnel": "yarn lt --port 4200 --local-https true --allow-invalid-cert -o",
    "debug": "yarn concurrently --kill-others \"yarn debugger\" \"yarn tunnel\"",
    "startinsecure": "yarn ng serve --host=0.0.0.0 --disable-host-check",
    "start": "yarn ng serve --ssl true --host=0.0.0.0 --disable-host-check",
    "build": "yarn ng build --configuration production",
    "build:electron": "export BROWSERSLIST_ENV=\"electron\" && yarn ng build -c electron",
    "build:watch": "export BROWSERSLIST_ENV=\"electron\" && yarn ng build -c electron --watch",
    "test": "yarn ng test",
    "lint": "yarn ng lint",
    "e2e": "yarn ng e2e",
    "build:stats": "yarn build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/gui/stats.json",
    "build:Azure": "yarn tsc --version && yarn ng --version && yarn build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.4",
    "@angular/cdk": "^11.2.3",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/forms": "~12.2.4",
    "@angular/material": "^11.2.3",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@angular/service-worker": "~12.2.4",
    "@azure/msal-browser": "^2.12.1",
    "@microsoft/microsoft-graph-client": "^2.2.1",
    "@types/file-saver": "^2.0.1",
    "canvas-chessboard": "0.0.17",
    "file-saver": "^2.0.2",
    "ngx-quill": "^14.3.0",
    "prosemirror-model": "^1.11.0",
    "quill": "^1.3.7",
    "rtf.js": "^3.0.1",
    "rxjs": "~6.6.6",
    "tslib": "^2.3.1",
    "typescript": "4.3.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.4",
    "@angular-devkit/build-angular": "^12.2.4",
    "@angular-devkit/core": "^12.2.4",
    "@angular-devkit/schematics": "^12.2.4",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@angular/language-service": "~12.2.4",
    "@microsoft/microsoft-graph-types": "^1.33.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.0.0",
    "cypress": "^8.3.1",
    "electron": "^14.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "localtunnel": "*",
    "ng-packagr": "^12.2.1",
    "protractor": "~7.0.0",
    "remotedebug-ios-webkit-adapter": "*",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}

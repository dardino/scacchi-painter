{
  "name": "sp-gui-angular",
  "version": "0.1.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "debugger": "pnpm remotedebug_ios_webkit_adapter --port 9000",
    "tunnel": "pnpm lt --port 4200 --local-https true --allow-invalid-cert -o",
    "debug": "pnpm concurrently --kill-others \"pnpm debugger\" \"pnpm tunnel\"",
    "startinsecure": "pnpm ng serve --host=0.0.0.0 --disable-host-check",
    "start:pwa": "pnpm start --configuration production",
    "start": "pnpm ng serve --ssl true --host=0.0.0.0 --disable-host-check",
    "build": "pnpm ng build gui --configuration production",
    "build:tauri": "env-cmd -f .tauri.env pnpm ng build gui -c tauri",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm ng lint",
    "e2e": "pnpm ng e2e",
    "build:stats": "pnpm build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/gui/stats.json",
    "build:Azure": "pnpm tsc --version && pnpm ng version && pnpm build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "^21.0.5",
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/forms": "^21.0.6",
    "@angular/material": "^21.0.5",
    "@angular/platform-browser": "^21.0.6",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@angular/router": "^21.0.6",
    "@angular/service-worker": "^21.0.6",
    "@azure/data-tables": "^13.3.0",
    "@azure/functions": "^4.11.2",
    "@azure/msal-angular": "^4.0.12",
    "@azure/msal-browser": "^4.27.0",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@standard-schema/spec": "^1.0.0",
    "@tauri-apps/api": "^2.6.0",
    "canvas-chessboard": "^0.0.17",
    "file-saver": "^2.0.5",
    "marked": "17.0.1",
    "marked-base-url": "^1.1.3",
    "ngx-editor": "18.0.0",
    "prosemirror-model": "^1.21.0",
    "rtf.js": "^3.0.9",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.0"
  },
  "pnpm": {
    "overrides": {
      "prosemirror-model": "1.21.0"
    }
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.0",
    "@angular-devkit/architect": "^0.2100.4",
    "@angular-devkit/build-angular": "^21.0.4",
    "@angular-devkit/core": "^21.0.4",
    "@angular-devkit/schematics": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@angular/language-service": "^21.0.6",
    "@eslint/js": "^9.39.2",
    "@microsoft/microsoft-graph-types": "^2.6.0",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@types/eslint-scope": "^8.3.0",
    "@types/file-saver": "^2.0.3",
    "@types/node": "25.0.3",
    "@types/wicg-file-system-access": "2023.10.6",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "angular-eslint": "21.1.0",
    "baseline-browser-mapping": "^2.9.19",
    "concurrently": "9.1.2",
    "env-cmd": "^11.0.0",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "61.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "globals": "^16.1.0",
    "jsdom": "^27.2.0",
    "localtunnel": "^2.0.2",
    "ng-packagr": "^21.0.0",
    "remotedebug-ios-webkit-adapter": "^0.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.33.1",
    "vite-node": "^5.2.0",
    "vitest": "^4.0.15"
  },
  "packageManager": "pnpm@9.15.0"
}

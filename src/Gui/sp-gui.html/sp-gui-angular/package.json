{
  "name": "sp-gui-angular",
  "version": "0.0.13",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "debugger": "yarn remotedebug_ios_webkit_adapter --port 9000",
    "tunnel": "yarn lt --port 4200 --local-https true --allow-invalid-cert -o",
    "debug": "yarn concurrently --kill-others \"yarn debugger\" \"yarn tunnel\"",
    "startinsecure": "yarn ng serve --host=0.0.0.0 --disable-host-check",
    "start:pwa": "yarn start --configuration production",
    "start": "yarn ng serve --ssl true --host=0.0.0.0 --disable-host-check",
    "build": "yarn ng build gui --configuration production",
    "build:tauri": "export BROWSERSLIST_ENV=\"tauri\" && yarn ng build -c tauri",
    "build:watch": "export BROWSERSLIST_ENV=\"electron\" && yarn ng build -c electron --watch",
    "test": "yarn ng test",
    "test:ci": "yarn ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "yarn ng lint",
    "e2e": "yarn ng e2e",
    "build:stats": "yarn build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/gui/stats.json",
    "build:Azure": "yarn tsc --version && yarn ng version && yarn build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.1",
    "@angular/cdk": "^20.1.2",
    "@angular/common": "^20.1.1",
    "@angular/compiler": "^20.1.1",
    "@angular/core": "^20.1.1",
    "@angular/forms": "^20.1.1",
    "@angular/material": "^20.1.2",
    "@angular/platform-browser": "^20.1.1",
    "@angular/platform-browser-dynamic": "^20.1.1",
    "@angular/router": "^20.1.1",
    "@angular/service-worker": "^20.1.1",
    "@azure/data-tables": "^13.3.0",
    "@azure/functions": "^4.7.1-preview",
    "@azure/msal-angular": "^4.0.12",
    "@azure/msal-browser": "^4.12.0",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@tauri-apps/api": "^2.6.0",
    "canvas-chessboard": "^0.0.17",
    "file-saver": "^2.0.5",
    "marked": "15.0.11",
    "marked-base-url": "^1.1.3",
    "ngx-editor": "18.0.0",
    "prosemirror-model": "^1.21.0",
    "rtf.js": "^3.0.9",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.0",
    "zone.js": "^0.15.0"
  },
  "resolutions": {
    "prosemirror-model": "1.21.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2001.1",
    "@angular-devkit/build-angular": "^20.1.1",
    "@angular-devkit/core": "^20.1.1",
    "@angular-devkit/schematics": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.1",
    "@angular/language-service": "^20.1.1",
    "@eslint/js": "^9.27.0",
    "@microsoft/microsoft-graph-types": "^2.6.0",
    "@types/eslint-scope": "^8.3.0",
    "@types/file-saver": "^2.0.3",
    "@types/jasmine": "5.1.8",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "22.15.19",
    "@types/wicg-file-system-access": "2023.10.6",
    "angular-eslint": "20.1.1",
    "concurrently": "9.1.2",
    "eslint": "^9.28.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "50.6.17",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "globals": "^16.1.0",
    "jasmine-core": "^5.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "localtunnel": "^2.0.2",
    "ng-packagr": "^20.1.0",
    "remotedebug-ios-webkit-adapter": "^0.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.33.1",
    "vite-node": "^3.1.4",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "packageManager": "yarn@4.9.2"
}

{
  "name": "sp-gui-angular",
  "version": "0.0.0",
  "scripts": {
    "debugger": "yarn remotedebug_ios_webkit_adapter --port 9000",
    "tunnel": "yarn lt --port 4200 --local-https true --allow-invalid-cert -o",
    "debug": "yarn concurrently --kill-others \"yarn debugger\" \"yarn tunnel\"",
    "startinsecure": "yarn ng serve --host=0.0.0.0 --disable-host-check",
    "start": "yarn ng serve --ssl true --host=0.0.0.0 --disable-host-check",
    "build": "yarn ng build --configuration production",
    "build:electron": "export BROWSERSLIST_ENV=\"electron\" && yarn ng build -c electron",
    "build:watch": "export BROWSERSLIST_ENV=\"electron\" && yarn ng build -c electron --watch",
    "test": "yarn ng test",
    "lint": "yarn ng lint",
    "e2e": "yarn ng e2e",
    "build:stats": "yarn build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/gui/stats.json",
    "build:Azure": "yarn tsc --version && yarn ng --version && yarn build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.6",
    "@angular/cdk": "^12.2.6",
    "@angular/common": "~12.2.6",
    "@angular/compiler": "~12.2.6",
    "@angular/core": "~12.2.6",
    "@angular/forms": "~12.2.6",
    "@angular/material": "^12.2.6",
    "@angular/platform-browser": "~12.2.6",
    "@angular/platform-browser-dynamic": "~12.2.6",
    "@angular/router": "~12.2.6",
    "@angular/service-worker": "~12.2.6",
    "@azure/msal-browser": "^2.17.0",
    "@microsoft/microsoft-graph-client": "^3.0.0",
    "@types/file-saver": "^2.0.3",
    "canvas-chessboard": "0.0.17",
    "file-saver": "^2.0.5",
    "ngx-quill": "^14.3.0",
    "prosemirror-model": "^1.14.3",
    "quill": "^1.3.7",
    "rtf.js": "^3.0.7",
    "rxjs": "~7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.6",
    "@angular-devkit/build-angular": "^12.2.6",
    "@angular-devkit/core": "^12.2.6",
    "@angular-devkit/schematics": "^12.2.6",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.6",
    "@angular/language-service": "~12.2.6",
    "@microsoft/microsoft-graph-types": "^2.6.0",
    "@types/jasmine": "~3.9.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.1",
    "cypress": "^8.4.1",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsdoc": "36.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "localtunnel": "2.0.2",
    "ng-packagr": "^12.2.2",
    "protractor": "~7.0.0",
    "remotedebug-ios-webkit-adapter": "*",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.3",
    "typescript": "^4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
